{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./src/assets/*"

      ],
      "@components/*":[
        "./src/components/*"

      ],
      "@routes/*":[
        "./src/routes/*"

      ],
      "@storage/*":[
        "./src/storage/*"

      ],
      "@screens/*":[
        "./src/screens/*"

      ],
      "@theme/*":[
        "./src/theme/*"

      ],
      "@utils/*":[
        "./src/utils/*"

      ],
    }
  }
}
